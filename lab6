a=[]
for i in range (3):
    a.append([x*0 for x in range(5)])
print(a)
def dollarfunction(astring):
    alist=astring.split()
    blist=[]
    for i in alist:
        blist.append('$'+i)
    return' '.join(blist) 
print(dollarfunction('this is the input'))
